{
  "name": "portfolio-client",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "bun run test:e2e && bun run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:e2e": "playwright test",
    "test:unit": "vitest",
    "kill": "sudo lsof -t -i:5173 | xargs sudo kill -9"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.46.0",
    "@skeletonlabs/skeleton": "^2.10.2",
    "@skeletonlabs/tw-plugin": "^0.2.4",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.4",
    "@types/cookie": "^0.5.4",
    "@types/google.accounts": "^0.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.43.0",
    "portfolio-api": "workspace:server",
    "portfolio-common": "workspace:common",
    "postcss": "^8.4.41",
    "postcss-load-config": "^4.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.18",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.8.5",
    "svelte-heros-v2": "^0.10.12",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "vite": "^4.5.3",
    "vitest": "^0.32.4"
  },
  "type": "module",
  "dependencies": {
    "@elysiajs/eden": "^0.7.7"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}